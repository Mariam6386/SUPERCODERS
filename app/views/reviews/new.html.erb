<div class="container">
<div class ="header-container">
 <h1 class="header-h1"><i class="fa-solid fa-mask"></i> <strong>Job Reviews</strong> <i class="fa-solid fa-mask"></i></h1>
<%= simple_form_for([@job, @review]) do |f| %>
  <%= f.input :rating, input_html: { min: '1', max: '5' } %>
  <%= f.input :comment %>
  <%= f.submit "Submit", class: "btn-flat"%>
<% end %>
</div>
</div>
